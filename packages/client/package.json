{
  "name": "client",
  "version": "1.0.0",
  "description": "客户端营销推送页面",
  "main": "main.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./scripts/index.js",
    "dev": "rimraf dist && cross-env NODE_ENV=production node ./scripts/dev.js",
    "build": "rimraf dist && cross-env NODE_ENV=production node ./scripts/index.js",
    "build:dll": "webpack --progress --config ./build/webpack.dll.js",
    "build:upload": "node ./scripts/upload.js",
    "test": "http-server ./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,.tsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --ignore-path .eslintignore",
      "git add "
    ]
  },
  "author": "harhao",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@sentry/cli": "1.74.3",
    "@sentry/react": "^7.43.0",
    "@sentry/tracing": "^7.43.0",
    "@sentry/webpack-plugin": "^1.20.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "address": "^1.2.2",
    "autoprefixer": "^9.7.4",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.4.2",
    "dotenv": "^16.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "git-rev-sync": "^3.0.2",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "log-symbols": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^4.4.0",
    "sass": "^1.58.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.4.2",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.8.0"
  }
}
